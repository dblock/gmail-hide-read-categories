{
  "name": "gmail-hide-read-categories",
  "version": "0.1.1",
  "description": "A GMail extension that hides read categories.",
  "private": true,
  "license": "MIT",
  "scripts": {
    "watch": "webpack --mode=development --watch --config config/webpack.config.js",
    "build": "webpack --mode=production --config config/webpack.config.js",
    "pack": "node pack.js",
    "repack": "npm run build && npm run pack",
    "format": "prettier --write --ignore-unknown \"{config,public,src}/**/*.{html,css,js,ts,json}\"",
    "clean": "rm -rf build release",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix"
  },
  "dependencies": {
    "gmail-js": "^1.1.14",
    "jquery": "^4.0.0-beta"
  },
  "devDependencies": {
    "adm-zip": "^0.5.16",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.11.0",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^2.9.2",
    "prettier": "^3.0.0",
    "webpack": "^5.98.0",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "^5.10.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3"
  }
}
